{
    "name": "osayaweventures/share-links",
    "description": "Social sharing links for your Statamic site",
    "type": "statamic-addon",
    "license": "MIT",
    "authors": [
        {
            "name": "Osayawe Ventures",
            "homepage": "https://github.com/osayaweventures"
        }
    ],
    "require": {
        "statamic/cms": ">=3.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Osayaweventures\\ShareLinks\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Share Links",
            "description": "Share Links addon"
        },
        "laravel": {
            "providers": [
                "Osayaweventures\\ShareLinks\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
